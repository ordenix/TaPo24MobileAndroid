<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="pl.tapo24.twa.ui.road.sign.signDetails.SignDetailsFragment">
    <TextView
            android:text="Szczegóły znaku A-1"
            style="@style/textStyleOrangeMediumWarpXl"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/signTitle"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:src="@tools:sample/avatars" android:id="@+id/imageView16"
            app:layout_constraintTop_toBottomOf="@+id/signTitle" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            style="@style/smallContainer"
            android:background="@drawable/style_small_container"
            android:layout_height="wrap_content" app:layout_constraintTop_toBottomOf="@+id/imageView16"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:id="@+id/constraintLayout9">

        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent">
            <TextView
                    android:text="@string/sign_desc"
                    style="@style/textStyleWhiteInContainerCenter"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView73"/>
            <View
                    android:layout_width="match_parent"
                    android:layout_height="3dp"
                    android:background="@drawable/style_orange_container"
                    app:layout_constraintTop_toBottomOf="@+id/constraintLayout7"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" android:layout_marginEnd="8dp" android:id="@+id/view13"/>
            <TextView
                    android:text="TextView"
                    style="@style/textStyleWhiteSmallInContainerCenter"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/signDescr"/>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
            style="@style/smallContainer"
            android:background="@drawable/style_small_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" app:layout_constraintTop_toBottomOf="@+id/constraintLayout9"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent">
        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
            <TextView
                    android:text="TextView"
                    style="@style/textStyleWhiteInContainerCenter"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView75"/>
            <View
                    android:layout_width="match_parent"
                    android:layout_height="3dp"
                    android:background="@drawable/style_orange_container"
                    app:layout_constraintTop_toBottomOf="@+id/constraintLayout7"
                    app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="8dp" android:layout_marginEnd="8dp" android:id="@+id/view14"/>
            <TextView
                    android:text="TextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView77"/>
            <TextView
                    android:text="TextView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" android:id="@+id/textView76"/>
            <Button
                    android:text="@string/more"
                    android:theme="@style/OrangeBtn"
                    android:backgroundTint="@color/orange_tapo"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:id="@+id/button7"
                    app:icon="@drawable/angles_right_solid"
                    app:iconSize="10dp"
                    app:layout_constraintTop_toBottomOf="@+id/divider5"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                    android:textSize="10dp" app:layout_constraintVertical_bias="1.0" android:layout_gravity="center"/>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>