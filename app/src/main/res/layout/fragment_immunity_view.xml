<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.road.immunity.immunityVIew.ImmunityViewFragment">
    <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:fillViewport="true">
        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                style="@style/smallContainer"
                android:background="@drawable/style_small_container"
                android:layout_height="wrap_content">

            <TextView
                    android:text="TextView"
                    style="@style/textStyleOrangeSmallWarpCenter"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" android:id="@+id/condition"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>
            <TextView
                    android:text="TextView"
                    style="@style/textStyleWhiteSmallInContainer"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" android:id="@+id/list"
                    app:layout_constraintTop_toBottomOf="@+id/condition" app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.core.widget.NestedScrollView>
</FrameLayout>