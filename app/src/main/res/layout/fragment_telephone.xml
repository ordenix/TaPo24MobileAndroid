<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent" android:id="@+id/main">
    <androidx.gridlayout.widget.GridLayout
            android:layout_width="match_parent"
            app:useDefaultMargins="true"
            android:layout_height="wrap_content"
            app:columnCount="3" app:layout_constraintTop_toBottomOf="@+id/appBarLayout"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/grid">

        <androidx.constraintlayout.widget.ConstraintLayout
                android:background="@drawable/style_grid_element"
                app:layout_columnWeight="1"
                app:layout_rowWeight="1" android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/linkTelephone">

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent">

                <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp" app:srcCompat="@drawable/telephone"
                        android:id="@+id/imageViewA"
                        app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"/>
                <TextView
                        android:text="@string/telephoneBook"
                        style="@style/gridText"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView19"
                        app:layout_constraintTop_toBottomOf="@+id/imageViewA"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
                android:background="@drawable/style_grid_element"
                app:layout_columnWeight="1"
                app:layout_rowWeight="1" android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/others">

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent">

                <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp" app:srcCompat="@drawable/triangle_exclamation_solid"
                        android:id="@+id/imageView81"
                        app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"/>
                <TextView
                        android:text="@string/numbers_other"
                        style="@style/gridText"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView191"
                        app:layout_constraintTop_toBottomOf="@+id/imageView81"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
                android:background="@drawable/style_grid_element"
                app:layout_columnWeight="1"
                app:layout_rowWeight="1" android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/itd">

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent">

                <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp" app:srcCompat="@drawable/truck_solid"
                        android:id="@+id/imageView82778"
                        app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"/>
                <TextView
                        android:text="@string/itd"
                        style="@style/gridText"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView192778"
                        app:layout_constraintTop_toBottomOf="@+id/imageView82778"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
                android:background="@drawable/style_grid_element"
                app:layout_columnWeight="1"
                app:layout_rowWeight="1" android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/sg">

            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent">

                <ImageView
                        android:layout_width="50dp"
                        android:layout_height="50dp" app:srcCompat="@drawable/earth_europe_solid"
                        android:id="@+id/imageView8278"
                        app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"/>
                <TextView
                        android:text="@string/sg"
                        style="@style/gridText"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView19278"
                        app:layout_constraintTop_toBottomOf="@+id/imageView8278"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.gridlayout.widget.GridLayout>
    <com.google.android.material.appbar.AppBarLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            android:paddingStart="10dp" android:paddingEnd="10dp" android:paddingBottom="5dp"
            android:paddingTop="5dp" android:id="@+id/appBarLayout">
        <com.google.android.material.textfield.TextInputLayout
                android:id="@+id/outlinedTextField"
                android:layout_width="match_parent"
                style="@style/Widget.App.TextInputLayout"
                android:layout_height="wrap_content"
                app:startIconDrawable="@drawable/magnifying_glass_solid"
                android:hint="@string/search_number">

            <com.google.android.material.textfield.TextInputEditText
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:maxLines="1" android:nestedScrollingEnabled="false" android:singleLine="true"
                    android:paddingTop="0dp" android:paddingBottom="0dp" android:id="@+id/queryText"/>

        </com.google.android.material.textfield.TextInputLayout>
    </com.google.android.material.appbar.AppBarLayout>
    <androidx.recyclerview.widget.RecyclerView
            android:layout_width="match_parent"
            android:layout_height="0dp" app:layout_constraintTop_toBottomOf="@+id/grid"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" tools:visibility="gone" android:id="@+id/rv"/>
</androidx.constraintlayout.widget.ConstraintLayout>