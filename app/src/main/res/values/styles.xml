<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="itimTextViewStyle">
        <item name="android:fontFamily">@font/itim</item>
    </style>
    <style name="textStyle">
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/itim</item>
        <item name="android:layout_marginEnd">1dp</item>
        <item name="android:maxLines">1</item>
    </style>
    <style name="textStyleLarge" parent="@style/textStyle">
        <item name="android:textSize">25sp</item>
    </style>
    <style name="textAboutApplicationLeftCol" parent="@style/textStyle">
        <item name="android:textAlignment">textEnd</item>
    </style>
    <style name="textAboutApplicationRightCol" parent="@style/textStyle">
        <item name="android:textAlignment">textStart</item>
    </style>
    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:background">@color/orange_tapo</item>
    </style>
    <style name="OrangeBtn">
        <item name="background">@color/orange_tapo</item>

    </style>
</resources>